<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>  
<class  name="com.appcloud.vm.action.dbentity.OltpTestResult" table="oltp_test_result" catalog="appcloud_performance" >

      <id name="id" column="id" type="int">
      </id>

      <property name="vmId" column="vm_id" type="int" />
      <property name="testTime" column="test_time" type="java.sql.Timestamp" />
      <property name="transactionFrq" column="transaction_frq" type="float" />
      <property name="deadLockFrq" column="dead_lock_frq" type="float" />
      <property name="readWriteFrq" column="read_write_frq" type="float" />
      <property name="otherOperationFrq" column="other_operation_frq" type="float" />
      <property name="totalTime" column="total_time" type="float" />
      <property name="totalEvent" column="total_event" type="int" />
      <property name="totalEventTime" column="total_event_time" type="float" />
      <property name="responseTimeMin" column="response_time_min" type="float" />
      <property name="responseTimeAvg" column="response_time_avg" type="float" />
      <property name="responseTimeMax" column="response_time_max" type="float" />
      <property name="responseTimeApprox" column="response_time_approx" type="float" />
      <property name="threadsFairnessEvent" column="threads_fairness_event" type="java.lang.String" />
      <property name="threadsFairnessExcTime" column="threads_fairness_exc_time" type="java.lang.String" />

</class>
</hibernate-mapping> 

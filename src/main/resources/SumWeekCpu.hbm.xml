<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- <?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
	低版本的用上面的，在不联网的时候就会报错，联网时启动特别慢 -->

<hibernate-mapping>
	<class name="com.appcloud.vm.action.dbentity.SumWeekCpu" table="cpu_test_result_newweek"
		catalog="appcloud_performance">

		<id name="id" column="id" type="int">
		</id>

		<property name="uuid" column="uuid" type="int" />
		<property name="companyname" column="companyname" type="java.lang.String" />
		<property name="time" column="time" type="java.lang.String" />
		<property name="totalTime" column="totalTime" type="float" />
		<property name="count" column="count" type="int" />

	</class>
</hibernate-mapping> 

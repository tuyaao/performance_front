<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>  
<class  name="com.appcloud.vm.action.dbentity.SumWeekPing" table="ping_test_result_newweek" catalog="appcloud_performance" >

        <id name="id" column="id" type="int"></id>

		<property name="uuid" column="uuid" type="int" />
		<property name="cpu" column="cpu" type="int" />
		<property name="mem" column="mem" type="int" />
		<property name="disk" column="disk" type="int" />
		<property name="bandwidth" column="bandwidth" type="int" />
		<property name="ip" column="ip" type="java.lang.String" />
		<property name="mac" column="mac" type="java.lang.String" />
		<property name="createTime" column="create_time" type="java.sql.Timestamp" />
		<property name="updateTime" column="update_time" type="java.sql.Timestamp" />
		<property name="os" column="os" type="java.lang.String" />
		<property name="companyId" column="company_id" type="int" />
		<property name="companyName" column="company_name" type="java.lang.String" />
		
		<property name="sumTime" column="sum_time" type="java.sql.Timestamp" />
		<property name="destIP" column="dest_ip" type="java.lang.String" />
		<property name="loss" column="loss" type="float" />
		<property name="avg" column="avg" type="float" />
		<property name="count" column="count" type="int" />
      
</class>
</hibernate-mapping> 
